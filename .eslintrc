{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "semi": [2, "never"],
    "strict": 0,
    "quotes": [ 1, "single" ],
    "no-undef": false,
    "global-strict": false,
    "no-extra-semi": 1,
    "no-underscore-dangle": false
  }
}
